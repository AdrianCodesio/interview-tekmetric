# Environment variables template for Docker Compose
# Copy this file to .env and update with your actual values

# Database Configuration
MYSQL_ROOT_PASSWORD=your_root_password_here
MYSQL_DATABASE=tekmetric_db
MYSQL_USER=tekmetric_user
MYSQL_PASSWORD=your_secure_password_here

# Application Configuration
SPRING_PROFILES_ACTIVE=prod
DB_HOST=mysql
DB_PORT=3306
DB_NAME=tekmetric_db
DB_USERNAME=tekmetric_user
DB_PASSWORD=your_secure_password_here
JWT_SECRET=your-very-long-secure-jwt-secret-key-here-at-least-32-characters